17.	public static void revers(int number) 
{
	    if (number < 10)
    {
	        System.out.println(number);
	        return;
	    }
	    else 
    {
	        System.out.print(number % 10);
	        revers(number/10);
	    }
}
18.
public  void middle(LinkedList l)
	{
		int counter=0;
		Node Temp=head;
		while(Temp!=null)
		{
			counter++;
			Temp=Temp.link;
		}
			Node temp2=head;
			int middle=counter/2;
			middle=middle+1;
			for(int i=0;i<middle-1;i++)
			{
				temp2=temp2.link;
			}
			System.out.println(temp2.data);
		}
		
